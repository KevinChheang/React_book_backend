{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "username": "john",
            "isbn": "978-1593279509",
            "is_like": false,
            "is_dislike": false
        }
    ],
    "required": [
        "username",
        "isbn",
        "is_like",
        "is_dislike"
    ],
    "properties": {
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The username schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "john"
            ]
        },
        "isbn": {
            "$id": "#/properties/isbn",
            "type": "string",
            "title": "The isbn schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "978-1593279509"
            ]
        },
        "is_like": {
            "$id": "#/properties/is_like",
            "type": "boolean",
            "title": "The is_like schema",
            "description": "An explanation about the purpose of this instance.",
            "default": false,
            "examples": [
                false
            ]
        },
        "is_dislike": {
            "$id": "#/properties/is_dislike",
            "type": "boolean",
            "title": "The is_dislike schema",
            "description": "An explanation about the purpose of this instance.",
            "default": false,
            "examples": [
                false
            ]
        }
    },
    "additionalProperties": true
}